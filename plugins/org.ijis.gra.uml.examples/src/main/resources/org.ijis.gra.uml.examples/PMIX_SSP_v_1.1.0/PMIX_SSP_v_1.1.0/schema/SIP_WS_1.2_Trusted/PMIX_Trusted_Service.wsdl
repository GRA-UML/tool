      <soap:operation soapAction="http://my.org/schema/SIP_WS_1.2_Trusted/pmp/WsBasicProfile/ReceiveDeferredPrescriptionDrugHistory" style="document"/>
      <input name="DeferredResponseType">
        <soap:body use="literal"/>
        <soap:header/>
        <soap:header/>
      </input>
      <output name="AcknowledgementType">
        <soap:body use="literal"/>
        <soap:header/>
      </output>
    </operation>
  </binding>
  <service name="pmpService">
