  </types>
  <message name="RequestType">
    <part element="tns2:RequestType" name="parameters"/>
  </message>
  <message name="ResponseType">
    <part element="tns2:ResponseType" name="parameters"/>
  </message>
  <message name="AcknowledgementType">
    <part element="tns2:AcknowledgementType" name="parameters"/>
  </message>
  <portType name="pmp">
