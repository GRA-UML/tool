<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:sp="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy" xmlns:tns="http://my.org/schema/SIP_WS_1.2_Trusted/PMIX_Trusted_Service" xmlns:tns2="http://www.pmix.gov" xmlns:wsaws="http://www.w3.org/2006/05/addressing/wsdl" xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy" xmlns:wsrm="http://schemas.xmlsoap.org/ws/2005/02/rm/policy" xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" name="PMIX_Trusted_Service" targetNamespace="http://my.org/schema/SIP_WS_1.2_Trusted/PMIX_Trusted_Service" xmlns="http://schemas.xmlsoap.org/wsdl/">
  <wsp:Policy wsu:Id="WsBasicProfile_policy">
