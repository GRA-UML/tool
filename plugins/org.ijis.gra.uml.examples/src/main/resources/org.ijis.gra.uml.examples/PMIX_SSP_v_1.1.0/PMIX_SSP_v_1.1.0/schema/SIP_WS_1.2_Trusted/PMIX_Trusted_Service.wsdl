      <output message="tns:ResponseType" name="ResponseType"/>
    </operation>
    <operation name="ReceiveDeferredPrescriptionDrugHistory">
      <input message="tns:ResponseType" name="DeferredResponseType"/>
      <output message="tns:AcknowledgementType" name="AcknowledgementType"/>
    </operation>
  </portType>
  <binding name="WsBasicProfileWsBasicProfileBinding" type="tns:pmp">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
