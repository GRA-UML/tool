<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:CAT="http://it.ojp.gov/gist/200/Corrections/category" xmlns:PLAN="http://it.ojp.gov/gist/200/Corrections/plan" xmlns:Q1="http://domain.gov/myDomain/" xmlns:i="http://niem.gov/niem/appinfo/2.0" xmlns:i2="http://niem.gov/niem/appinfo/2.1" xmlns:nc="http://niem.gov/niem/niem-core/2.0" xmlns:niem-xsd="http://niem.gov/niem/proxy/xsd/2.0" xmlns:s="http://niem.gov/niem/structures/2.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://it.ojp.gov/gist/200/Corrections/plan" version="&quot;1&quot;">
  <xsd:import namespace="http://it.ojp.gov/gist/200/Corrections/category" schemaLocation="../../../XMLschemas/Content/extension/CategoryExtension.xsd"/>
  <xsd:import namespace="http://domain.gov/myDomain/" schemaLocation="../../../XMLschemas/Content/extension/EntityExtension.xsd"/>
  <xsd:import namespace="http://niem.gov/niem/niem-core/2.0" schemaLocation="../../../XMLschemas/Content/subset/niem/2.0/niem-core.xsd"/>
  <xsd:import namespace="http://niem.gov/niem/appinfo/2.0" schemaLocation="../../../XMLschemas/appinfo/2.0/appinfo.xsd"/>
  <xsd:import namespace="http://niem.gov/niem/proxy/xsd/2.0" schemaLocation="../../../XMLschemas/proxy/xsd/2.0/xsd.xsd"/>
  <xsd:import namespace="http://niem.gov/niem/appinfo/2.1" schemaLocation="../../../XMLschemas/appinfo/2.1/appinfo.xsd"/>
  <xsd:import namespace="http://niem.gov/niem/structures/2.0" schemaLocation="../../../XMLschemas/structures/2.0/structures.xsd"/>
  <xsd:complexType abstract="false" mixed="false" name="PlanType">
    <xsd:annotation>
      <xsd:documentation>A data type for A plan is a planned series of activities to achieve some purpose relative to a subject.</xsd:documentation>
      <xsd:appinfo>
        <i2:AugmentedTypeIndicator>true</i2:AugmentedTypeIndicator>
        <i:Base i:name="Object" i:namespace="http://niem.gov/niem/structures/2.0"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" ref="PLAN:EntityIdentification"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="PLAN:PlanGoalDescriptionText"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="PLAN:PlanStrategyDescriptionText"/>
          <xsd:element maxOccurs="unbounded" minOccurs="1" ref="PLAN:PlanManager"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="PLAN:PlanStrategyStatus"/>
          <xsd:element maxOccurs="1" minOccurs="0" ref="PLAN:PlanTargetCompletionDate"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" ref="PLAN:PlanStrengthIndicator"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element abstract="false" name="EntityIdentification" nillable="false" substitutionGroup="s:Augmentation" type="Q1:EntityAugmentationType">
    <xsd:annotation>
      <xsd:documentation>Identifical of a plan</xsd:documentation>
      <xsd:documentation>A Documented Component</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="PlanGoalDescriptionText" nillable="false" type="nc:TextType">
    <xsd:annotation>
      <xsd:documentation>Description of the plan's goals</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="PlanStrategyDescriptionText" nillable="false" type="nc:TextType">
    <xsd:annotation>
      <xsd:documentation>Description of the plan's strategy</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="PlanManager" nillable="false" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>Manager of the plan</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="PersonType" i:namespace="http://niem.gov/niem/niem-core/2.0"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="PlanStrategyStatus" nillable="false" type="CAT:StatusType">
    <xsd:annotation>
      <xsd:documentation>Status relevent to the strategies paln</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="PlanTargetCompletionDate" nillable="false" type="nc:DateType">
    <xsd:annotation>
      <xsd:documentation>Date the plan should be completed</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="PlanStrengthIndicator" nillable="false" type="niem-xsd:boolean">
    <xsd:annotation>
      <xsd:documentation>Needs review**</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType abstract="false" mixed="false" name="RemediationPlanType">
    <xsd:annotation>
      <xsd:documentation>A data type for A data type representing a remediation plan. A plan is a series of planned activities.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="PlanType" i:namespace="http://it.ojp.gov/gist/200/Corrections/plan"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="PLAN:PlanType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType abstract="false" mixed="false" name="ReentryPlanType">
    <xsd:annotation>
      <xsd:documentation>A data type for a reentry plan</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="PlanType" i:namespace="http://it.ojp.gov/gist/200/Corrections/plan"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="PLAN:PlanType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType abstract="false" mixed="false" name="PlanActivitiesAssociationType">
    <xsd:annotation>
      <xsd:documentation>A relationship A Documented Component</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="Association" i:namespace="http://niem.gov/niem/structures/2.0"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="s:ComplexObjectType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="1" ref="PLAN:PlanForActivity"/>
          <xsd:element maxOccurs="1" minOccurs="1" ref="PLAN:PlannedActivity"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element abstract="false" name="PlanForActivity" nillable="false" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A plan for this activity</xsd:documentation>
      <xsd:documentation>A Documented Component</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="PlanType" i:namespace="http://it.ojp.gov/gist/200/Corrections/plan"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element abstract="false" name="PlannedActivity" nillable="false" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>A Documented Component</xsd:documentation>
      <xsd:documentation>Activity planned</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="ActivityType" i:namespace="http://niem.gov/niem/niem-core/2.0"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType abstract="false" mixed="false" name="EmploymentPlanType">
    <xsd:annotation>
      <xsd:documentation>A data type for A data type representing an employment plan. A plan is a series of planned activities.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="PlanType" i:namespace="http://it.ojp.gov/gist/200/Corrections/plan"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="PLAN:PlanType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType abstract="false" mixed="false" name="HousingPlanType">
    <xsd:annotation>
      <xsd:documentation>A data type for A data type representing a housing plan. A plan is a series of planned activities.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="PlanType" i:namespace="http://it.ojp.gov/gist/200/Corrections/plan"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="PLAN:PlanType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType abstract="false" mixed="false" name="MedicalConditionPlanType">
    <xsd:annotation>
      <xsd:documentation>A data type for A data type representing a medicaln plan. A plan is a series of planned activities.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="PlanType" i:namespace="http://it.ojp.gov/gist/200/Corrections/plan"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="PLAN:PlanType">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" minOccurs="1" ref="PLAN:PlanForCondition"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element abstract="false" name="PlanForCondition" nillable="false" type="s:ReferenceType">
    <xsd:annotation>
      <xsd:documentation>Plan for a medical condition</xsd:documentation>
      <xsd:appinfo>
        <i:ReferenceTarget i:name="MedicalConditionType" i:namespace="http://niem.gov/niem/niem-core/2.0"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType abstract="false" mixed="false" name="EducationPlanType">
    <xsd:annotation>
      <xsd:documentation>A data type for A data type representing an education plan. A plan is a series of planned activities.</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="PlanType" i:namespace="http://it.ojp.gov/gist/200/Corrections/plan"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="PLAN:PlanType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType abstract="false" mixed="false" name="CasePlanType">
    <xsd:annotation>
      <xsd:documentation>A data type for I data type representing a case plan</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:name="PlanType" i:namespace="http://it.ojp.gov/gist/200/Corrections/plan"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="PLAN:PlanType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:annotation>
    <xsd:appinfo>
      <i:ConformantIndicator>true</i:ConformantIndicator>
    </xsd:appinfo>
  </xsd:annotation>
</xsd:schema>
