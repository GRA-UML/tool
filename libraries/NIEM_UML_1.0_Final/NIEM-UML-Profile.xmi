          <type href="http://www.omg.org/spec/UML/20110701/UML.xmi#Property"> </type>
          <name>base_Property</name>
          <visibility>private</visibility>
          <association xmi:idref="NIEM_UML_Profile-NIEM_PIM_Profile-packagedElement-2"/>
        </ownedAttribute>
        <name>RoleOf</name>
      </packagedElement>
