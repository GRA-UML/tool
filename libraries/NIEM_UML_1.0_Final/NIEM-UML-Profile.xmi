            <annotatedElement
              xmi:idref="NIEM_UML_Profile-NIEM_Common_Profile-NIEMType-NDR__Rule_9-32_"/>
          </ownedComment>
          <name>NDR [Rule 9-32]</name>
          <specification
            xmi:id="NIEM_UML_Profile-NIEM_Common_Profile-NIEMType-NDR__Rule_9-32_-specification"
